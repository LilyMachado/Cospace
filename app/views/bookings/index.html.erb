<h1>Bookings</h1>
<%= link_to 'New Booking', new_booking_path %>
<ul>
  <% @bookings.each do |booking| %>
    <li>
      <%= link_to "Booking ##{booking.id}", booking %>
      <%= link_to 'Edit', edit_booking_path(booking) %>
      <%= link_to 'Delete', booking, method: :delete, data: { confirm: 'Are you sure?' } %>
    </li>
  <% end %>
</ul>
